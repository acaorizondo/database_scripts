                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">DECLARE @TodayDate DATE;
SET @TodayDate = GETDATE();

DECLARE @database_name varchar(50);
SET @database_name = 'AdventureWorks2022';
	
SELECT CAST(@database_name AS varchar(50)) AS database_name, 
              CAST(t.object_id AS varchar(50)) AS table_id,
              t.name AS table_name,
              @TodayDate as date,
              YEAR(@TodayDate) * 10000 + MONTH(@TodayDate) * 100 + DAY(@TodayDate) AS DateKey,
             SUM(p.rows) AS row_count
FROM sys.tables t INNER JOIN sys.partitions p ON t .object_id = p.object_id
WHERE p.index_id IN (0, 1)
GROUP BY t .object_id, t .name
ORDER BY  row_count DESC;</property>
