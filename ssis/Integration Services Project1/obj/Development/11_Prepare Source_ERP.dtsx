            // TODO: Add your code here
            string filePath = Dts.Variables["User::CurrentFile"].Value.ToString();

            // Encontrar el �ndice del primer car�cter num�rico en el nombre del archivo
            int startIndex = filePath.IndexOfAny("0123456789".ToCharArray());

            // Si se encuentra un car�cter num�rico, extraer la parte de la fecha
            if (startIndex != -1)
            {
                // Extraer la parte de la fecha del nombre del archivo
                string datePart = filePath.Substring(startIndex, 10);

                // Reemplazar guiones bajos con nada para obtener un formato que DateTime.ParseExact pueda entender
                datePart = datePart.Replace("_", "");

                // Convertir la parte de la fecha a un objeto DateTime
                DateTime fileDate = DateTime.ParseExact(datePart, "yyyyMMdd", System.Globalization.CultureInfo.InvariantCulture);

                // Obtener la fecha actual menos 2 d�as
                DateTime currentDateMinusTwoDays = DateTime.Now.AddDays(-3);

                // Eliminar el archivo si la fecha del archivo es anterior a currentDateMinusTwoDays
                if (fileDate < currentDateMinusTwoDays)
                {
                    System.IO.File.Delete(filePath);
                }
            }



            Dts.TaskResult = (int)ScriptResults.Success;
		}

