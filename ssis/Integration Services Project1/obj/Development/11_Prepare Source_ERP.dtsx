            // TODO: Add your code here
            Boolean newBackup = (bool)Dts.Variables["User::NewBackup"].Value;
            if (newBackup)
            {
                try
                {
                    string origen = Dts.Variables["$Package::VirtualFilePathBackupSource"].Value.ToString();
                    string destino = Dts.Variables["User::localBackupFolder"].Value.ToString();
                    string archivo = Dts.Variables["$Package::FileName"].Value.ToString();

                    // Construir rutas completas
                    string rutaCompletaOrigen = Path.Combine(origen, archivo);
                    string rutaCompletaDestino = Path.Combine(destino, archivo);

                    // Verificar si el archivo de origen existe
                    if (File.Exists(rutaCompletaOrigen))
                    {
                        // Copiar el archivo
                        File.Copy(rutaCompletaOrigen, rutaCompletaDestino, true);

                        // Éxito
                        Dts.TaskResult = (int)ScriptResults.Success;
                        return;
                    }
                    else
                    {
                        // Registro de error si el archivo de origen no existe
                        Dts.Events.FireError(0, "Script Task Error", $"El archivo de origen no existe: {rutaCompletaOrigen}", string.Empty, 0);

